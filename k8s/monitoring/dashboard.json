            "matcher": { "id": "byName", "options": "CPU %" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 1 },
              { "id": "custom.cellOptions", "value": { "type": "gauge", "mode": "gradient" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 60 },
                { "color": "red", "value": 85 }
              ]}}
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Memory %" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "decimals", "value": 1 },
              { "id": "custom.cellOptions", "value": { "type": "gauge", "mode": "gradient" } },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 70 },
                { "color": "red", "value": 90 }
              ]}}
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Memory Used" },
            "properties": [{ "id": "unit", "value": "bytes" }]
          },
          {
            "matcher": { "id": "byName", "options": "CPU Cores" },
            "properties": [{ "id": "decimals", "value": 2 }]
          },
          {
            "matcher": { "id": "byName", "options": "Status" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "type": "color-text" } },
              { "id": "mappings", "value": [
                { "options": { "Running": { "color": "green", "text": "üü¢ Running" } }, "type": "value" },
                { "options": { "Pending": { "color": "yellow", "text": "üü° Pending" } }, "type": "value" },
                { "options": { "Failed": { "color": "red", "text": "üî¥ Failed" } }, "type": "value" }
              ]}
            ]
          }
        ]
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "id": 10,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": false, "displayName": "Pod" }]
      },
      "pluginVersion": "10.2.0",
      "targets": [
        {
          "expr": "sum by (pod) (rate(container_cpu_usage_seconds_total{namespace=\"tavana\",pod=~\"worker-.*\",container!=\"\",container!=\"POD\"}[5m])) * 100",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "CPU"
        },
        {
          "expr": "sum by (pod) (container_memory_working_set_bytes{namespace=\"tavana\",pod=~\"worker-.*\",container!=\"\",container!=\"POD\"})",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "Memory"
        },
        {
          "expr": "sum by (pod) (container_memory_working_set_bytes{namespace=\"tavana\",pod=~\"worker-.*\",container!=\"\",container!=\"POD\"}) / sum by (pod) (kube_pod_container_resource_limits{namespace=\"tavana\",pod=~\"worker-.*\",resource=\"memory\"}) * 100",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "MemPct"
        },
        {
          "expr": "kube_pod_status_phase{namespace=\"tavana\",pod=~\"worker-.*\",phase=\"Running\"} * 1 or kube_pod_status_phase{namespace=\"tavana\",pod=~\"worker-.*\",phase=\"Pending\"} * 0",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "Status"
        }
      ],
      "title": "Worker Pod Details",
      "transformations": [
        { "id": "merge", "options": {} },
        { 
          "id": "organize", 
          "options": { 
            "excludeByName": { "Time": true, "Time 1": true, "Time 2": true, "Time 3": true, "Time 4": true, "__name__": true, "namespace": true, "phase": true, "container": true, "endpoint": true, "instance": true, "job": true, "node": true, "service": true, "uid": true },
            "renameByName": { 
              "pod": "Pod",
              "Value #CPU": "CPU Cores",
              "Value #Memory": "Memory Used",
              "Value #MemPct": "Memory %",
              "Value #Status": "Status"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "reduceRow",
            "reduce": { "include": ["CPU Cores"], "reducer": "lastNotNull" },
            "alias": "CPU %",
            "replaceFields": false
          }
        }
      ],
      "type": "table"
    },
